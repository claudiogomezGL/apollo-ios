// Search Paths
ALWAYS_SEARCH_USER_PATHS = NO
HEADER_SEARCH_PATHS = $(inherited) $(CONFIGURATION_BUILD_DIR)
FRAMEWORK_SEARCH_PATHS = $(inherited) $(CONFIGURATION_BUILD_DIR)
FRAMEWORK_SEARCH_PATHS[sdk=appletv*] = $(inherited) $(CONFIGURATION_BUILD_DIR) $(SRCROOT)/Carthage/Build/tvOS
FRAMEWORK_SEARCH_PATHS[sdk=iphone*] = $(inherited) $(CONFIGURATION_BUILD_DIR) $(SRCROOT)/Carthage/Build/iOS
